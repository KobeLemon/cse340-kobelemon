<h2 class="loginTitle"><%- title %></h2>
<% messages() %>
<form class="loginForm">
  <label for="account_email">Email:</label>
  <input type="email" id="account_email" placeholder="email@domain.com" required>

  <label for="account_password">Password:</label>
  <input type="text" id="account_password" placeholder="Password" required>

  <button type="submit" id="submit">Login</button>

  <p>No account yet? <a href="/" id="signUpLink">Sign Up!</a></p>
</form>
